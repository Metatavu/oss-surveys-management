{
  "name": "oss-management",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --open",
    "tsc-dev": "tsc --noEmit && vite --open",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "build-client": "openapi-generator-cli generate -i oss-surveys-api-spec/swagger.yaml -o src/generated/client -c generator-config.json -g typescript-fetch",
    "lint:check": "rome ci src/",
    "lint:fix": "rome check src/ --apply-suggested && rome format src/ --write",
    "postinstall": "npm run build-client"
  },
  "dependencies": {
    "@emotion/react": "11.10.6",
    "@emotion/styled": "11.10.6",
    "@mui/icons-material": "5.11.11",
    "@mui/material": "5.11.14",
    "@mui/x-date-pickers": "6.5.0",
    "@react-pdf/renderer": "3.1.12",
    "@sentry/react": "7.45.0",
    "@types/lodash.isequal": "4.5.6",
    "envalid": "7.3.1",
    "html-to-image": "1.11.11",
    "html2canvas": "1.4.1",
    "jotai": "2.0.3",
    "keycloak-js": "21.0.1",
    "localized-strings": "0.2.4",
    "lodash.isequal": "4.5.0",
    "luxon": "3.3.0",
    "moment": "2.29.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-router-dom": "6.9.0",
    "react-toastify": "9.1.3",
    "recharts": "2.6.2",
    "sanitize-html": "2.10.0",
    "usehooks-ts": "2.9.1",
    "uuid": "9.0.0"
  },
  "devDependencies": {
    "@openapitools/openapi-generator-cli": "2.5.2",
    "@types/luxon": "3.2.0",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "@types/sanitize-html": "2.9.0",
    "@types/uuid": "9.0.1",
    "@vitejs/plugin-react": "3.1.0",
    "rome": "11.0.0",
    "typescript": "5.0.2",
    "vite": "4.2.1"
  }
}
