{
  "name": "oss-management",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --open",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "build-client": "openapi-generator-cli generate -i oss-surveys-api-spec/swagger.yaml -o src/generated/client -c generator-config.json -g typescript-fetch",
    "lint:check": "rome ci src/",
    "lint:fix": "rome check src/ --apply-suggested && rome format src/ --write"
  },
  "dependencies": {
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@mui/icons-material": "^5.11.11",
    "@mui/material": "^5.11.14",
    "@sentry/react": "^7.44.2",
    "envalid": "^7.3.1",
    "localized-strings": "^0.2.4",
    "moment": "^2.29.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.9.0",
    "jotai": "2.0.3",
    "keycloak-js": "21.0.1"
  },
  "devDependencies": {
    "@openapitools/openapi-generator-cli": "^2.5.2",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@vitejs/plugin-react": "^3.1.0",
    "rome": "^11.0.0",
    "typescript": "^4.9.3",
    "vite": "^4.2.0"
  }
}